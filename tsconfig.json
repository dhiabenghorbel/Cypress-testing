{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6", "dom"],
    "types": ["cypress"],
    "strict": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "*": ["types/*"]
    }
  },
  "include": ["**/*.ts", "../../node_modules/cypress", "cypress/**/*.ts", "node_modules/cypress/types/index.d.ts", "node_modules/cypress-skip-test/index.d.ts"]
}
